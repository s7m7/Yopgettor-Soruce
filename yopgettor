ECHO %ESC%[31m and sort them alphabetical.
ECHO %ESC%[31m Please drag and drop your combo file in the same folder
ECHO %ESC%[31m and drop the name of it below (without .txt)
set /p name=">"
sort /c /unique "%name%.txt" /o "output.txt"
