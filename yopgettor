ECHO %ESC%[31m Databse: %datanom%
ECHO %ESC%[31m Password: %pass%
ECHO %ESC%[31m Result File Name: %path%
set linecount=%datalong%
set /a randnum=%random% %% %linecount%+1
set tempnum=1
For /f "delims=" %%L in ('type %datanom%.txt') do (
    if !tempnum! equ %randnum% (
        set name=%%L
        goto next
        )
    set /a tempnum+=1
    )
:next
set /a z = %_length%
set "string=abcdefghijklmnopqrstuvwxyz0123456789_-"
set "user="
for /L %%i in (1,1,!z!) do call :_genRand
echo %name%%user%@%domai%:%pass% >> %path%.txt
goto loopbehindall

:_genRand
set /a x=%random% %% 62
set user=%user%!string:~%x%,1!
goto :eof

