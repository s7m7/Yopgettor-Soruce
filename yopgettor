ECHO %ESC%[34m ?8(  d88  88b  d88  88b  d8P88b  ,88b 88b      88b     88b   88b  d88 d88     
ECHO %ESC%[34m `?88P'?8b `?8888P'  888888P'`?88P'`88b`?888P'  `?8b    `?8b  `?8888P'd88'     
ECHO %ESC%[34m        )88          88P'           )88                                        
ECHO %ESC%[34m       ,d8P         d88            ,88P                                        
ECHO %ESC%[34m    `?888P'         ?8P        `?8888P                                                        
ECHO %ESC%[31m Made with ♥ by Samu#5111
ECHO %ESC%[31m _______________________________________________________________________________ 
ECHO %ESC%[31m [1] Get YOPmails
ping 127.0.0.1 -n 1 -w 500 >nul
ECHO %ESC%[31m [2] Combo Editor
ping 127.0.0.1 -n 1 -w 500 >nul
ECHO %ESC%[31m [3] Get YOPmails with Database
ping 127.0.0.1 -n 1 -w 500 >nul
ECHO %ESC%[31m [4] Info and Updates
ping 127.0.0.1 -n 1 -w 500 >nul
ECHO %ESC%[31m [TUTORIAL] Full Tutorial
set /p promenu=" >"

if %promenu% == 1 goto maker
if %promenu% == 2 goto comboeditor
if %promenu% == 3 goto database
if %promenu% == 4 goto other
if %promenu% == tutorial goto tutorial
if %promenu% == Tutorial goto tutorial
if %promenu% == TUTORIAL goto tutorial

:error3
cls
goto pro

:maker
echo %number%
cls
color 1
setlocal enableextensions enabledelayedexpansion
set /P _length=Mail max. length (5-15 recommended): %==%
cls
set /P domain="Yopmail Domain (Yopmail.com/Yopmail.fr/Yopmail.net):
cls
set /P pass="Yopmail Password > "
cls
set /P path="Results File Name > "
cls
title YOPgettor Public v3.0 Made with ♥ by Samu#5111 // Getting Yopmails // Default Mode
ECHO %ESC%[34m                                                 d8P     d8P                   
ECHO %ESC%[34m                                              d888888Pd888888P                 
ECHO %ESC%[34m ?88   d8P  d8888b ?88,.d88b, d888b8b   d8888b  ?88'    ?88'   d8888b   88bd88b
ECHO %ESC%[34m d88   88  d8P' ?88`?88'  ?88d8P' ?88  d8b_,dP  88P     88P   d8P' ?88  88P'  `
ECHO %ESC%[34m ?8(  d88  88b  d88  88b  d8P88b  ,88b 88b      88b     88b   88b  d88 d88     
ECHO %ESC%[34m `?88P'?8b `?8888P'  888888P'`?88P'`88b`?888P'  `?8b    `?8b  `?8888P'd88'     
ECHO %ESC%[34m        )88          88P'           )88                                        
ECHO %ESC%[34m       ,d8P         d88            ,88P                                        
ECHO %ESC%[34m    `?888P'         ?8P        `?8888P                                                        
ECHO %ESC%[31m Made with ♥ by Samu#5111
ECHO %ESC%[31m ________________________________________________________________________________________
ECHO %ESC%[31m Mode: Default
ECHO %ESC%[31m Databse: -
ECHO %ESC%[31m Password: %pass%
ECHO %ESC%[31m Result File Name: %path%
:loop
set /a z = %_length%
set "string=abcdefghijklmnopqrstuvwxyz0123456789-_"
set "user="
for /L %%i in (1,1,!z!) do call :_genRand
echo %user%@%domain%:%pass% >> %path%.txt
goto loop

:_genRand
set /a x=%random% %% 62
set user=%user%!string:~%x%,1!
goto :eof


